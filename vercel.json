{
    "version": 2,
    "builds": [
      {
        "src": "api/relate.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "cd frontend && npm install && npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/relate",
        "dest": "/api/relate.py"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/$1"
      }
    ]
  }
  